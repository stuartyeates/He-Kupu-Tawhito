%.orig.xml:
	wget http://www.nzetc.org/etexts/$*.xml --output-document=$@
	sleep 1

%.fixedurls.xml: %.orig.xml
	xsltproc  ../../xsl/tei2NZETCURLs.xsl $^  | xsltproc ../../xsl/fix-structure.xsl  - > $@

%.words.xml: %.fixedurls.xml
	saxonb-xslt  $^ ../../xsl/tei2teiWords.xsl  > $@

all: copyold bilingualfiles

copyold:
	cp ../../../he_kupu_tawhito-old/korero/www.nzetc.org/*.orig.*xml .


bilingualfiles: $(bilingualfiles)

clean:
	rm -rf *.html *.fixedurls.xml *.words.xml

allclean: clean	
	rm -rf *.orig.xml bilingualfiles allfiles

tei-corpus-%.html:
	wget http://www.nzetc.org/tm/scholarly/$^

#bilingualfiles: tei-corpus-legalMaoriBodies.html tei-corpus-legalMaoriAgreement.html tei-corpus-legalMaoriCommunity.html tei-corpus-legalMaoriCourt.html tei-corpus-legalMaoriCrown.html tei-corpus-legalMaoriStatutory.html 
#	cat tei-corpus-legalMaori* | tr '<> ' '\012' | grep href | grep 'href="tei-' | sort | uniq | sed 's/[^-]*-//' | sed 's/.html"//' | awk '{print $$1".words.xml"}' > $@

allfiles: tei-doc.html
	cat tei-doc.html | tr '<> ' '\012' | grep href | grep 'href="tei-' | sort | uniq | sed 's/[^-]*-//' | sed 's/.html"//' | awk '{print $$1".words.xml"}' > $@

validate: 
	xmllint --noout *.xml

.PRECIOUS: %.orig.xml
